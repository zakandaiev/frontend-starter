    "rollup": "^4.12.0",
    "rollup-plugin-multi-input": "^1.4.1",
    "sass": "^1.71.0",
    "stylelint": "^16.2.1",
    "stylelint-config-clean-order": "^5.4.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-scss": "^6.1.0"
