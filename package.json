{
  "name": "frontend-starter",
  "version": "2.6.0",
  "private": false,
  "author": "Oleksandr Zakandaiev",
  "authorUrl": "https://github.com/zakandaiev",
  "description": "FrontEnd Starter is a boilerplate kit for easy building modern static web-sites using Gulp",
  "keywords": [
    "gulp",
    "builder",
    "frontend",
    "starter",
    "boilerplate",
    "kit",
    "twig",
    "html",
    "css",
    "sass",
    "scss",
    "javascript",
    "js",
    "es6",
    "optimized",
    "autoprefixer",
    "rollup",
    "browsersync",
    "404",
    "grid",
    "htaccess",
    "robots.txt",
    "sitemap.xml",
    "favicon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zakandaiev/frontend-starter"
  },
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "imports": {
    "#root/*": "./*",
    "#core/*": "./core/*",
    "#src/*": "./src/*"
  },
  "browserslist": [
    "last 15 versions",
    "not dead"
  ],
  "scripts": {
    "dev": "gulp",
    "build": "gulp --build",
    "preview": "gulp preview --build",
    "docs": "gulp --build --base=/frontend-starter --dist=./docs",
    "lint:js": "eslint ./",
    "lint:js:fix": "eslint ./ --fix",
    "lint:css": "stylelint ./src/sass/**/*.{sass,scss}",
    "lint:css:fix": "stylelint ./src/sass/**/*.{sass,scss} --fix",
    "backend": "nodemon backend"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "autoprefixer": "^10.4.24",
    "browser-sync": "^3.0.4",
    "cssnano": "^7.1.2",
    "del": "^8.0.1",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "fastify": "^5.7.4",
    "globals": "^17.3.0",
    "gulp": "^5.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^9.2.0",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^6.0.1",
    "gulp-twig": "^1.2.0",
    "gulp-version-number": "^0.2.4",
    "localtunnel": "^2.0.2",
    "minimist": "^1.2.8",
    "nodemon": "^3.1.14",
    "postcss-combine-media-query": "^2.1.0",
    "rollup": "^4.58.0",
    "rollup-plugin-multi-input": "^1.7.0",
    "sass": "^1.97.3",
    "stylelint": "^17.3.0",
    "stylelint-config-clean-order": "^8.0.1",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-scss": "^7.0.0"
  },
  "dependencies": {
    "@tabler/icons": "^3.37.1",
    "reseter.css": "^2.0.0"
  },
  "overrides": {
    "eslint": "^9.39.2"
  }
}
