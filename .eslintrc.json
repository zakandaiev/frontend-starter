{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase": 0,
        "consistent-return": 0,
        "global-require": 0,
        "import/no-extraneous-dependencies": 0,
        "linebreak-style": 0,
        "max-len": 0,
        "no-param-reassign": 0,
        "no-promise-executor-return": 0,
        "no-undef": 0,
        "no-unneeded-ternary": 0,
        "no-unused-vars": 0,
        "no-use-before-define": 0
    }
}
